{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "jsx": "react",
    "outDir": "./dist/",
    "lib": [
      "es2017",
    ]
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules"
  ]
}
